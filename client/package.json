{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "devDependencies": {
    "autoprefixer": "^9.7.6",
    "babel-core": "^6.26.3",
    "concat": "^1.0.3",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.19.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "core-js": "^3.6.5",
    "lodash": "^4.17.15",
    "parcel-bundler": "^1.12.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-stripe-checkout": "^2.6.3",
    "redux": "^4.0.5",
    "redux-form": "^8.3.2",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "scss": "node-sass src/res/sass/main.scss src/res/css/style.css -w",
    "compile-scss": "node-sass src/res/sass/main.scss src/res/css/style.comp.css",
    "concat-css": "concat -o src/res/css/style.concat.css src/res/css/icon-font.css src/res/css/style.comp.css",
    "prefix-css": "postcss src/res/css/style.concat.css --use autoprefixer -o src/res/css/style.prefix.css",
    "compress-css": "node-sass src/res/css/style.prefix.css public/style.css --output-style compressed",
    "build-css": "npm-run-all compile-scss concat-css prefix-css compress-css",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch:js": "parcel watch ./src/index.js --out-dir ./public --out-file bundle.js",
    "build:js": "parcel build ./src/index.js --out-dir ./public --out-file bundle.js",
    "parcel:js": " parcel src/index.html --open"
  },
  "browserslist": [
    "last 10 version",
    "> 0.2%",
    "IE 10"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "development": [
    "last 1 chrome version",
    "last 1 firefox version",
    "last 1 safari version"
  ]
}
